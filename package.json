{
  "name": "html5-boilerplate",
  "version": "0.1.0",
  "description": "HTML5 Boilerplate is a professional front-end template for building fast, robust, and adaptable web apps or sites.",
  "main": "js/main.js",
  "scripts": {
    "prebuild": "eslint src test",
    "watch:test": "ava --watch",
    "pretest": "npm run build",
    "test": "ava",
    "prepublish": "npm test"
  },
  "ava": {
    "files": [
      "smart-grid/test.js",
      "!**/not-this-file.js"
    ],
    "source": [
      "**/*.js",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "verbose": true,
    "babel": "inherit"
  },
  "author": "Arvind Ravulavaru",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.4.2",
    "gulp": "^3.6.2",
    "gulp-connect": "^2.0.5",
    "jquery": "^3.1.0",
    "modernizr": "^3.3.1",
    "moment": "^2.14.1",
    "morphdom": "^1.4.6",
    "once": "^1.3.3",
    "page": "^1.7.1",
    "ramda": "^0.21.0",
    "toastr": "^2.1.2",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "ali-oss": "^4.4.4",
    "ava": "^0.15.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "co": "^4.6.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "gulp-npm-run": "^0.0.3",
    "gulp-rev": "^7.1.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "mocha": "^3.0.0",
    "mz": "^2.4.0",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.12.1",
    "rollup-plugin-commonjs": "^3.1.0",
    "rollup-plugin-node-resolve": "^1.7.1",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "webpack-stream": "^3.2.0"
  }
}
