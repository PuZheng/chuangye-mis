{
  "name": "html5-boilerplate",
  "version": "0.1.0",
  "description": "HTML5 Boilerplate is a professional front-end template for building fast, robust, and adaptable web apps or sites.",
  "main": "js/main.js",
  "scripts": {
    "prebuild": "eslint src test",
    "watch:test": "ava --watch",
    "test": "ava",
    "prepublish": "npm test"
  },
  "ava": {
    "files": [
      "smart-grid/test.js",
      "!**/not-this-file.js"
    ],
    "source": [
      "**/*.js",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "verbose": true,
    "babel": "inherit"
  },
